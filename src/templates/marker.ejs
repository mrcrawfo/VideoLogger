<li class="<%= getTypeName(type).toLowerCase() %>" ondblclick="editMarker(<%= index %>)">
    <div class="iconholder">
        <% switch(type) {
            case 1: %>
                <span class="glyphicon glyphicon-film" aria-hidden="true"></span> <%
                break;
            case 2: %>
                <span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> <%
                break;
            case 3: %>
                <span class="glyphicon glyphicon-globe" aria-hidden="true"></span> <%
                break;
            case 4: %>
                <span class="glyphicon glyphicon-flag" aria-hidden="true"></span> <%
                break;
            case 5: %>
                <span class="glyphicon glyphicon-knight" aria-hidden="true"></span> <%
                break;
        } %>
    </div>
    <div class="textholder">
        <strong><%= timecode %> - <%= getTypeName(type) %></strong>
        <p>Chapter <%= chapter %></p>
    </div>
    <button type="button" class="btn btn-default remove" onclick="removeMarker(<%= index %>)"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button>
</li>